{
  "name": "iata-fred",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "ng serve --proxy-config proxy.conf.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env2json npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.5",
    "@ngx-translate/core": "^8.0.0",
    "angular2-tippy": "^1.0.2",
    "angular2-tooltip": "^3.1.0",
    "bootstrap": "^4.0.0-beta",
    "core-js": "^2.5.1",
    "d3": "^4.11.0",
    "d3-ng2-service": "^1.21.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.4.7",
    "@angular/compiler-cli": "^4.4.5",
    "@angular/language-service": "^4.4.5",
    "@biesbjerg/ngx-translate-extract": "^2.3.2",
    "@ngx-rocket/scripts": "^1.0.5",
    "@types/d3": "^4.11.0",
    "@types/jasmine": "^2.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/lodash": "^4.14.77",
    "@types/node": "^8.0.44",
    "@types/topojson": "^3.0.2",
    "codelyzer": "~3.2.1",
    "hads": "^1.4.0",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~1.7.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "mdbootstrap": "^4.4.1",
    "phantomjs-prebuilt": "^2.1.15",
    "protractor": "^5.2.0",
    "stylelint": "^8.2.0",
    "stylelint-config-recommended-scss": "~2.0.0",
    "stylelint-config-standard": "~17.0.0",
    "stylelint-scss": "~2.1.0",
    "topojson": "^3.0.2",
    "ts-node": "^3.3.0",
    "tslint": "~5.7.0",
    "typescript": "^2.5.3"
  },
  "repository": "https://github.com/antonio-rodrigues/fred.git"
}
